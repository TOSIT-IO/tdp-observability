offline: true
exclude_paths:
  - .github/
  - dev/
  - docs/
  - venv

warn_list: []

skip_list: 
  - jinja[spacing]
  - name[missing]
  - name[play]
  - var-naming[no-reserved]
  - var-naming[no-role-prefix]
  - yaml[brackets]
  - yaml[commas]
  - yaml[comments]
  - yaml[empty-lines]
  - yaml[indentation]
  - yaml[trailing-spaces]
  - yaml[truthy]
  - name[template]
  - name[casing]
  - galaxy[no-changelog]
  - galaxy[no-runtime]
  - galaxy[tags]
  - galaxy[version-incorrect]

